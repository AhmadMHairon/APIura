{
    "name": "apiura/apiura",
    "description": "Interactive API documentation and testing tool for Laravel. Browse, test, save, and annotate your API endpoints from a single page.",
    "keywords": ["laravel", "api", "documentation", "testing", "openapi", "swagger"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Apiura\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apiura\\ApiuraServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
